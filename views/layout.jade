            - var title = apiInfo.name + ' API Documentation'
        -else
            - var title = 'Live Documentation'
        title=title
        link(rel='stylesheet', href='/stylesheets/prettify.css')
        link(rel='stylesheet', href='/stylesheets/style.css')
        link(rel='stylesheet', href='/javascripts/jquery-ui/themes/base/jquery.ui.all.css')
        link(rel='stylesheet', href='/javascripts/jquery-selectBox/jquery.selectBox.css')
        script(src='/javascripts/jquery-ui/jquery-1.7.1.js')

    body
        !{body}
        div(id='io-footer')
           div(class='f-left') &copy;  
              a(href='http://mashery.com/io') Mashery, Inc.
           div(class='f-right') Powered by 
              a(href='https://github.com/mashery/iodocs') I/O Docs
        script(src='/javascripts/json2.js')
        script(src='/javascripts/prettify.js')
        script(src='/javascripts/utilities.js')
        script(src='/javascripts/livedocs.js')
        script(src='/javascripts/docs.js')
        script(src='/javascripts/jquery-dateFormat/jquery.dateFormat-1.0.js')
        script(src='/javascripts/jquery-ui/ui/jquery.ui.core.js')
        script(src='/javascripts/jquery-ui/ui/jquery.ui.widget.js')
